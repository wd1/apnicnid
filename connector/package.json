{
  "name": "apnic-stats-connector",
  "version": "0.0.1",
  "description": "APNIC Stats Connector",
  "scripts": {
    "lint": "eslint .",
    "clear": "rm -rf dist",
    "prebuild": "npm run clear",
    "build": "babel scripts/ --out-dir dist/",
    "scratch": "cross-env NODE_ENV=development babel-node scripts/scratch.js",
    "backup": "./scripts/shell/backup.sh",
    "restore": "./scripts/shell/restore.sh",
    "bump": "ncu --upgradeAll; export PKG=eslint-config-airbnb; npm info \"$PKG@latest\" peerDependencies --json | command sed 's/[\\{\\},]//g ; s/: /@/g' | xargs npm install --save-dev \"$PKG@latest\"; npm install -d;",
    "development": "cross-env NODE_ENV=development babel-node scripts",
    "pretest": "cross-env NODE_ENV=test npm run build",
    "test": "cross-env NODE_ENV=test mocha \"dist/tests/**/*.test.js\"",
    "prestart": "npm run build",
    "start": "node dist"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-airbnb": "^2.3.3",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.4.2",
    "node-fetch": "^1.7.1",
    "npm-check-updates": "^2.12.0"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "bluebird": "^3.5.0",
    "cross-env": "^5.0.1",
    "csv": "^1.1.1",
    "fast-json-patch": "^2.0.2",
    "fs-extra": "^3.0.1",
    "ip-address": "^5.8.8",
    "jsbn": "^1.1.0",
    "klaw": "^1.3.1",
    "lodash": "^4.17.4",
    "mathjs": "^3.13.3",
    "moment-timezone": "^0.5.13",
    "mongodb": "^2.2.29",
    "unhandled-error": "^1.0.0",
    "validator": "^7.0.0",
    "winston": "^2.3.1",
    "winston-mongodb": "^2.0.10"
  }
}
