{
  "name": "apnic-stats-api",
  "version": "0.0.3",
  "description": "APNIC Stats API",
  "scripts": {
    "lint": "eslint .",
    "clear": "rm -rf dist",
    "prebuild": "npm run clear",
    "build": "babel scripts/ --out-dir dist/",
    "bump": "ncu --upgradeAll; export PKG=eslint-config-airbnb; npm info \"$PKG@latest\" peerDependencies --json | command sed 's/[\\{\\},]//g ; s/: /@/g' | xargs npm install --save-dev \"$PKG@latest\"; npm install -d;",
    "scratch": "cross-env NODE_ENV=development babel-node scripts/scratch.js",
    "development": "cross-env NODE_ENV=development nodemon --exec babel-node --watch scripts/ -e js,json scripts/index.js",
    "pretest": "cross-env NODE_ENV=test npm run build",
    "test": "cross-env NODE_ENV=test mocha \"dist/tests/**/*.test.js\"",
    "prestart": "npm run build",
    "start": "node dist"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "express-winston": "^2.4.0",
    "lodash": "^4.17.4",
    "mathjs": "^3.13.3",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mongodb": "^2.2.29",
    "node-uuid": "^1.4.8",
    "serve-favicon": "^2.4.3",
    "shortid": "^2.2.8",
    "unhandled-error": "^1.0.0",
    "validator": "^7.0.0",
    "winston": "^2.3.1",
    "winston-mongodb": "^2.0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-airbnb": "^2.3.3",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.4.2",
    "node-fetch": "^1.7.1",
    "nodemon": "^1.11.0",
    "npm-check-updates": "^2.12.0"
  }
}
