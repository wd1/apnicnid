      {{#each delegations.ipv6}}
      <tr>
        <td class="economy">{{this.code}}</td>
        <td class="blocks">{{this.new.count}}</td>
        <td class="delegations">{{this.new.sum}}</td>
      </tr>
      {{/each}}
    </tbody>
    {{/if}}
  </table>

</div>

