<div class="tabular delegations">

  <table>
    <caption>ASN</caption>
    <thead>
      <tr>
        <th scope="col">ECONOMY</th>
        <th scope="col">BLOCKS</th>
        <th scope="col">DELEGATIONS</th>
      </tr>
    </thead>
    {{#if delegations.asn}}
    <tbody>
      {{#each delegations.asn}}
      <tr>
        <td class="economy">{{this.code}}</td>
        <td class="blocks">{{this.new.count}}</td>
        <td class="delegations">{{this.new.sum}}</td>
      </tr>
      {{/each}}
    </tbody>
    {{/if}}
  </table>

  <table>
    <caption>IPv4</caption>
