<table>
  <tbody>
    <tr>
      <th>URL</th>
      <td>/jobs</td>
    </tr>
    <tr>
      <th>DESCRIPTION</th>
      <td>Fetches all connector jobs.</td>
    </tr>
    <tr>
      <th>EXAMPLE</th>
      <td>
        <a href="/jobs?status=completed,started,failed&amp;limit=1000&amp;sort=newest&amp;format=html">
          <span>/jobs?status=completed,started,failed&amp;limit=1000&amp;sort=newest&amp;format=html</span>
        </a>
      </td>
    </tr>
    <tr>
      <th>PARAMETERS</th>
      <td>
        <table class="parameters">
          <thead>
            <tr>
              <th scope="col">name</th>
              <th scope="col">description</th>
              <th scope="col">default</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>status</td>
              <td>Comma-separated list of job statuses.</td>
              <td>completed,started,failed</td>
            </tr>
            <tr>
              <td>limit</td>
              <td>Any unsigned integer.</td>
              <td>100</td>
            </tr>
            <tr>
              <td>sort</td>
              <td>Either "newest" or "oldest".</td>
              <td>newest</td>
            </tr>
            <tr>
              <td>format</td>
              <td>Either html or json.</td>
              <td>json</td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
  </tbody>
</table>
